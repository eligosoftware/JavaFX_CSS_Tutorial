<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@styles.css" fx:id="gridPane">
    <Button text="Open..." onAction="#handleClick" GridPane.rowIndex="0" GridPane.columnIndex="0"
    />
    <Button text="Button2" GridPane.rowIndex="0" GridPane.columnIndex="1"
            />
    <Button text="Button3" GridPane.rowIndex="0" GridPane.columnIndex="2"
            id="button_three"
            />
    <Button text="Button4" fx:id="button4" GridPane.rowIndex="0" GridPane.columnIndex="3"
            />
    <Button text="Button5" GridPane.rowIndex="0" GridPane.columnIndex="4"
            />
    <RadioButton text="Choice1" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <Label fx:id="label" rotate="90" onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit" text="JavaFX Effects" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    <Hyperlink text="http://www.javafx.com" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#handleLinkClick"/>
    <WebView fx:id="webView" GridPane.rowIndex="3" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowSpan="3"/>
</GridPane>